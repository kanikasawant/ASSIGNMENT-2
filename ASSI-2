a=int(input("enter a number"))
b=int(input("enter a number"))
print("a +b =",a+b)#add
print("a-b =",a-b)#sub
print("a % b =",a%b)#modulous
print(a==b)
print(a!=b)
print(a is b)
print(a is not b)
print(a>4 and b<13)
print("bitwise >>",a>>b)
print("bitwise <<",a<<b)
print("is",a is b)
print("is not",a is not b)
d=10
d+=1
print("+=",d)
y=10
y**=3
print("**=",y)
w="prince"
print('s' in w)
print("a" not in w)